{
  "name": "connecting",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "__dev": "nodemon --watch server.ts --exec node -r @swc-node/register ./server.ts",
    "dev": "next dev -H localhost --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir .",
    "lint:fix": "next lint --fix --dir .",
    "socket": "nodemon --watch socket.ts --watch src/server/socket --ext ts --exec node -r @swc-node/register ./socket.ts",
    "electron": "nodemon --watch electron.js --watch src/server/electron --exec electron ./electron.js"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "@prisma/extension-accelerate": "^1.3.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@swc/core": "^1.11.21",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.5.0",
    "jose": "^6.0.10",
    "mediasoup": "^3.15.7",
    "mediasoup-client": "^3.10.1",
    "next": "^15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swc-node": "^1.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "electron": "^36.2.0",
    "eslint": "^9.25.1",
    "eslint-config-next": "^15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "packageManager": "yarn@1.22.22"
}
