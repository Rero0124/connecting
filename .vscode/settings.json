{
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit",
		"source.fixAll.eslint": "explicit"
	},
	"eslint.validate": [
		"javascript",
		"typescript",
		"typescriptreact",
		"javascriptreact"
	],
	"editor.defaultFormatter": "dbaeumer.vscode-eslint",
	"eslint.useFlatConfig": true
}
